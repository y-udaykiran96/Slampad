
<div class="row">
    <div class="col m3"></div>
    <div class="col s12 m6">
    <ul class="collection">
    {{#each users}}
    <li class="collection-item avatar">
    <img src="{{this.image}}" alt="" class="circle">
    <span class="title">{{this.firstName}}</span>
    <p> {{this.email}}
    </p>
    <a href="/users/sendreq/{{this._id}}" class="secondary-content"><i class="fa fa-plus-square"></i></a>
    </li>
    {{/each}}
</ul>
    </div>
    <div class="col m3"></div>
</div>
